{
  "name": "garasi",
  "version": "0.0.0",
  "description": "Graphical User Interface for Garage S3",
  "homepage": "https://github.com/riipandi/garasi#readme",
  "bugs": {
    "url": "https://github.com/riipandi/garasi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riipandi/garasi.git"
  },
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "tsc -b --noEmit && bunx --bun vite build",
    "start": "PORT=3990 bun run --env-file=.env.local .output/server/index.mjs",
    "docker:build": "docker build -f Dockerfile . -t $npm_package_config_image:$npm_package_version-dev",
    "docker:push": "docker push $npm_package_config_image:latest-dev && docker push $npm_package_config_image:$npm_package_version-dev",
    "docker:run": "pnpm --silent run dockerx --name $npm_package_name $npm_package_config_image:$npm_package_version-dev",
    "docker:shell": "pnpm --silent run dockerx --entrypoint sh $npm_package_config_image:$npm_package_version-dev",
    "docker:prune": "docker image rm --force $(docker image list --filter reference=$npm_package_config_image:* -q)",
    "docker:images": "docker image list --filter reference=$npm_package_config_image:*",
    "docker:migrate": "pnpm --silent run dockerx --entrypoint goose $npm_package_config_image:$npm_package_version-dev",
    "dockerx": "docker run --network=host --rm -it --env-file=.env.local -v $(pwd)/storage:/srv/storage:rw",
    "compose:cleanup": "docker compose -f compose.yaml down --remove-orphans --volumes",
    "compose:up": "docker compose -f compose.yaml up --detach --remove-orphans",
    "compose:down": "docker compose -f compose.yaml down --remove-orphans",
    "cleanup": "pnpm dlx del-cli .{nitro,output,tanstack} bun.lock node_modules storybook-static",
    "update-deps": "npm-check-updates --configFileName .ncurc.json",
    "setup-dev": "dotenv -e .env.local -- sh ./scripts/setup-dev.sh",
    "garage:cmd": "dotenv -e .env.local -- sh ./scripts/garage.sh",
    "garage": "docker-compose exec garage /garage",
    "storybook:deps": "playwright install chromium --with-deps",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "typecheck": "tsc -b --noEmit --extendedDiagnostics",
    "check": "oxfmt --check && oxlint",
    "format": "oxfmt --write .",
    "lint": "oxlint --fix"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@loglayer/plugin-redaction": "^3.0.2",
    "@loglayer/transport-log-file-rotation": "^3.0.2",
    "@loglayer/transport-simple-pretty-terminal": "^3.0.2",
    "@nanostores/persistent": "^1.3.3",
    "@nanostores/react": "^1.0.0",
    "@tanstack/react-devtools": "^0.9.6",
    "@tanstack/react-form": "^1.28.3",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.161.4",
    "@tanstack/react-router-devtools": "^1.161.4",
    "@tanstack/react-table": "^8.21.3",
    "@upyo/core": "^0.4.0",
    "@upyo/smtp": "^0.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html-to-text": "^9.0.5",
    "jose": "^6.1.3",
    "kysely": "^0.28.11",
    "kysely-bun-sqlite": "^0.4.0",
    "loglayer": "^9.1.0",
    "lucide-react": "^0.575.0",
    "nanostores": "^1.1.0",
    "nuqs": "^2.8.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "serialize-error": "^13.0.1",
    "tailwind-merge": "^3.5.0",
    "tan-themer": "^0.4.1",
    "tw-animate-css": "^1.4.0",
    "typeid-js": "^1.2.0",
    "ua-parser-js": "^2.0.9",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.2.10",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-vitest": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@tailwindcss/vite": "^4.2.0",
    "@tanstack/devtools-vite": "^0.5.1",
    "@tanstack/router-plugin": "^1.161.4",
    "@types/bun": "^1.3.9",
    "@types/html-to-text": "^9.0.4",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv-cli": "^11.0.0",
    "nitro": "3.0.1-alpha.2",
    "npm-check-updates": "^19.4.1",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0",
    "playwright": "^1.58.2",
    "std-env": "^3.10.0",
    "storybook": "^10.2.10",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.3.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "config": {
    "image": "ghcr.io/riipandi/garasi"
  }
}
