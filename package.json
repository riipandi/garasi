{
  "name": "garasi",
  "version": "0.0.0",
  "description": "Graphical User Interface for Garage S3",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bun run typecheck && bunx --bun vite build",
    "start": "PORT=3980 bun run --env-file=.env.local .output/server/index.mjs",
    "docker:build": "docker build -f Dockerfile . -t $npm_package_config_image:$npm_package_version-dev",
    "docker:push": "docker push $npm_package_config_image:latest-dev && docker push $npm_package_config_image:$npm_package_version-dev",
    "docker:run": "pnpm --silent run dockerx --name $npm_package_name $npm_package_config_image:$npm_package_version-dev",
    "docker:shell": "pnpm --silent run dockerx --entrypoint sh $npm_package_config_image:$npm_package_version-dev",
    "docker:prune": "docker image rm --force $(docker image list --filter reference=$npm_package_config_image:* -q)",
    "docker:images": "docker image list --filter reference=$npm_package_config_image:*",
    "docker:migrate": "pnpm --silent run dockerx --entrypoint goose $npm_package_config_image:$npm_package_version-dev",
    "dockerx": "docker run --platform=linux/amd64 --network=host --rm -it --env-file=.env.local -v $(pwd)/storage:/srv/storage:rw",
    "compose:cleanup": "docker compose -f compose.yaml down --remove-orphans --volumes",
    "compose:up": "docker compose -f compose.yaml up --detach --remove-orphans",
    "compose:down": "docker compose -f compose.yaml down --remove-orphans",
    "cleanup": "pnpm dlx del-cli .{nitro,output,tanstack} dist bun.lock node_modules",
    "update-deps": "npm-check-updates --configFileName .ncurc.json",
    "genkey": "dotenv -e .env.local -- sh ./scripts/prepare.sh",
    "garage:cmd": "dotenv -e .env.local -- sh ./scripts/garage.sh",
    "garage": "docker-compose exec garage /garage",
    "check": "oxfmt --check && oxlint",
    "format": "oxfmt --write .",
    "lint": "oxlint --fix",
    "typecheck": "tsc -b --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@loglayer/plugin-redaction": "^2.1.15",
    "@loglayer/transport-log-file-rotation": "^2.1.16",
    "@loglayer/transport-simple-pretty-terminal": "^2.3.1",
    "@nanostores/persistent": "^1.2.0",
    "@nanostores/react": "^1.0.0",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.153.2",
    "@tanstack/react-router-devtools": "^1.153.2",
    "@tanstack/react-table": "^8.21.3",
    "@upyo/core": "^0.4.0",
    "@upyo/smtp": "^0.4.0",
    "jose": "^6.1.3",
    "kysely": "^0.28.10",
    "kysely-bun-sqlite": "^0.4.0",
    "loglayer": "^8.4.0",
    "lucide-react": "^0.562.0",
    "nanostores": "^1.1.0",
    "nuqs": "^2.8.6",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "std-env": "^3.10.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tw-animate-css": "^1.4.0",
    "typeid-js": "^1.2.0",
    "ua-parser-js": "^2.0.8",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/router-plugin": "^1.153.2",
    "@types/bun": "^1.3.6",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv-cli": "^11.0.0",
    "nitro": "3.0.1-alpha.1",
    "npm-check-updates": "^19.3.1",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.3.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "config": {
    "image": "ghcr.io/riipandi/garasi"
  }
}
